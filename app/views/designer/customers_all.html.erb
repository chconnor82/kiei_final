<div class="page-header">
  <h1>610 Home 'Guys Home' Customer Details</h1>
</div>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Room Preference</th>
      <th>Current Room (photo)</th>
      <th>Email</th>
      <th>Personal Syle</th>
      <th>Contact Preference</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
      <tr>
    <% User.all.each do |user| %>
      <tr>
        <td>
          <% room_preference = RoomPreference.find_by_session_id(user.session_id) %>
          <p><%= room_preference.integer %></p>
          <p><%= room_preference.room_photo_id %></p>
        </td>
    <% end %>

    <% User.all.each do |user| %>
        <td>
          <% current_room = CurrentRoom.find_by_session_id(user.session_id) %>

          <img src= "<%= current_room.room_image %>" width="100">
        </td>


        <td><%= current_room.room_type %></td>



        <td></td>
        <td></td>
        <td>
          <a href="" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
  </tbody>
</table>

