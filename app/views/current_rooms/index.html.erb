<img src="http://610homegeneva.com/wp-content/uploads/2010/03/610home-black.jpg" width="100">

<div class="page-header">
  <h1>Current rooms</h1>
</div>

<p><a href="/current_rooms/new" class="btn btn-primary">Add a New Current Room</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Height</th>
      <th>Width</th>
      <th>Room type</th>
      <th>Photo image</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @current_room.each do |current_room| %>
      <tr>
        <td><%= current_room.height %></td>
        <td><%= current_room.Width %></td>
        <td><%= current_room.room_type %></td>
        <td><img src= "<%= current_room.room_image %>" width="100"></td>

        <td>
          <a href="/current_rooms/<%= current_room.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/current_rooms/<%= current_room.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_current_room/<%= current_room.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

